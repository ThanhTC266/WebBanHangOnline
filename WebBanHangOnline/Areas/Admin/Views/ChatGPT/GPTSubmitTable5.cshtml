
@{
    ViewBag.Title = "GPTSubmitTable5";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h2>GPTSubmitTable5</h2>

<table id="myTable">
    <thead>
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th>Country</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>John</td>
            <td>30</td>
            <td>USA</td>
        </tr>
        <tr>
            <td>Jane</td>
            <td>25</td>
            <td>Canada</td>
        </tr>
        <tr>
            <td>Bob</td>
            <td>40</td>
            <td>UK</td>
        </tr>
    </tbody>
</table>
<button id="myButton">Get Table Data</button>

<script>
    $(document).ready(function () {
        $('#myButton').click(function () {
            var tableData = [];
            var headers = [];

            // Get table headers
            $('#myTable th').each(function () {
                headers.push($(this).text());
            });

            // Get table data
            $('#myTable tbody tr').each(function () {
                var rowData = {};
                $(this).find('td').each(function (index, element) {
                    rowData[headers[index]] = $(element).text();
                });
                tableData.push(rowData);
            });

            // Output the result
            console.log(tableData);
        });
    });

</script>